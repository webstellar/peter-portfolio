{
  "name": "peter-portfolio",
  "private": true,
  "description": "A portfolio website for all the projects Peter has worked on",
  "version": "0.1.0",
  "author": "Peter Onyegbule <pete.onyegbule@gmail.com>",
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "@react-icons/all-files": "^4.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "gatsby": "^5.8.1",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^3.8.0",
    "gatsby-plugin-manifest": "^5.8.0",
    "gatsby-plugin-postcss": "^6.8.0",
    "gatsby-plugin-sharp": "^5.8.0",
    "gatsby-source-filesystem": "^5.8.0",
    "gatsby-source-wordpress": "^7.8.0",
    "gatsby-transformer-sharp": "^5.8.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-tagcloud": "^2.3.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "prettier": "^2.8.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build:css": "postcss ./src/styles/global.css -o ./dist/output.css",
    "watch:css": "postcss ./src/styles/global.css -o ./dist/output.css -w",
    "css": "tailwindcss -i ./src/styles/global.css -o ./dist/output.css",
    "watch": "tailwindcss -i ./src/styles/global.css -o ./dist/output.css --watch",
    "build": "npm run build:css & gatsby build",
    "develop": "npm run watch:css & gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,css}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
